# Тест 4: Инициализация и вывод массива

# Инициализация массива
data = [64, 34, 25, 12, 22, 11, 90];
print("Исходный массив:");
print(data);

# Добавление элементов в массив
data.append(55);
print("Массив после добавления элемента 55:");
print(data);

# Удаление элемента из массива
data.remove(34);
print("Массив после удаления элемента 34:");
print(data);